@model object

<a asp-action="Details" asp-route-id="@Model" class="btn btn-primary">Details</a>
@if (User.Identity!.IsAuthenticated && User.IsInRole("Administrator"))
{
    <a asp-action="Edit" asp-route-id="@Model" class="btn btn-warning">Edit</a>
    <a asp-action="Delete" asp-route-id="@Model" class="btn btn-danger">Delete</a>
}